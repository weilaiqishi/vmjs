{
  "name": "@kimeng/vm",
  "version": "0.5",
  "description": "Javascript Interpreter, run Javascript code in ECMAScript",
  "main": "./dist/vm.js",
  "source": "src",
  "typings": "./dist/vm.d.ts",
  "scripts": {
    "test": "rm -rf ./build && tsc -p ./ && nyc ava ./build/test ./build/src/**/*.test.js",
    "build": "rm -rf ./dist && webpack --display-error-details --progress --colors",
    "buildNRM": "webpack --display-error-details --progress --colors",
    "format": "prettier --write ./src/**/*.ts ./test/**/*.ts",
    "changelog": "npx conventional-changelog-cli -p angular -i CHANGELOG.md -s -r 0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/axetroy/vm.js.git"
  },
  "author": "Axetroy",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.23.6",
    "@babel/plugin-transform-modules-commonjs": "^7.23.3",
    "@babel/plugin-transform-strict-mode": "^7.23.3",
    "@babel/types": "^7.23.6",
    "lodash.isfunction": "^3.0.9",
    "ts-loader": "^9.5.1"
  },
  "bugs": {
    "url": "https://github.com/axetroy/vm.js/issues"
  },
  "homepage": "https://github.com/axetroy/vm.js#readme",
  "devDependencies": {
    "@types/lodash": "4.14.168",
    "@types/node": "*",
    "ava": "^6.0.1",
    "coveralls": "3.1.0",
    "husky": "5.0.8",
    "nyc": "15.1.0",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3",
    "webpack": "5.89.0",
    "webpack-cli": "3.3.12"
  }
}
